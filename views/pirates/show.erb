<h1>Pirate</h1>

<h2>Name: <%= @pirate.name %></h2><br>
<h2>Weight: <%= @pirate.weight %></h2><br>
<h2>Height: <%= @pirate.height %></h2><br>

<h1>Ship</h1>
<% @ships.each do |ship| %>
    <p>Name: <%= ship.name %></p><br>
    <p>Type: <%= ship.type %></p><br>
    <p>Booty: <%= ship.booty %></p>
<% end %>

